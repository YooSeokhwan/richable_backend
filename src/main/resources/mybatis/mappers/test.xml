<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.idle.kb_i_dle_backend">
    <select id="selectUser" resultType="com.idle.kb_i_dle_backend.community.dto.BoardDTO">
        SELECT *
        FROM users
        WHERE id = #{id}
    </select>
</mapper>
